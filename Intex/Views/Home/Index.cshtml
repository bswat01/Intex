
@{
    ViewBag.Title = "Home";
}
<div id="jump1" class="jumper"></div>
<!-- Services -->
<div class="section type-2">
    <div class="container">
        <div class="section-headlines">
            <h4>North West Labs - What we're doing</h4>
            <div> Check out the latest in pharmacology and our work with the origin, nature, chemistry, effects, and uses of drugs </div>
        </div>
        <div class="row">
            <div class="col-lg-6 features text-center"> <img src="~/images/NWLogo.jpeg" alt="" /> </div>
            <div class="col-lg-6 features">
                <div class="media">
                    <i class=" icon-wrench pull-left animated bounce"></i>
                    <div class="media-body">
                        <h3>Testing</h3>
                        <div>We offer a variety of tests for all types of compounds.</div>
                    </div>
                </div>
                <br class="gap-30" />
                <div class="media">
                    <i class=" icon-bar-chart pull-left animated bounce"></i>
                    <div class="media-body">
                        <h3>Detailed and accurate reports</h3>
                        <div>Any report that we generate will contain everything there is to know and more.</div>
                    </div>
                </div>
                <br class="gap-30" />
                <div class="media">
                    <i class=" icon-mobile-phone pull-left animated bounce"></i>
                    <div class="media-body">
                        <h3>Advanced research techniques</h3>
                        <div>Our research and testing techniques are the most advanced in the industry. Let our experience help you.</div>
                    </div>
                </div>
                <br class="gap-30" />
                <div class="media">
                    <i class=" icon-mobile-phone pull-left animated bounce"></i>
                    <div class="media-body">
                        <h3>Fastest results possible</h3>
                        <div>We deliver accurate results on all types of compounds faster than any of our competitor all while ensuring the best quality.</div>
                    </div>
                </div>
                <div class="visible-xs visible-sm">
                    <br class="gap-30" />
                    <hr class="gap-divider" />
                    <br class="gap-30" />
                </div>
            </div>
        </div>
        <br>
        <br>
        <br>
        <div class="row">
            <div class="col-lg-4 features">
                <h3>Our Services</h3>
                <div class="skills">
                    <p>Catalogs</p>
                    <div class="progress">
                        <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 70%;"> </div>
                    </div>
                    <hr>
                    <p>Testing</p>
                    <div class="progress">
                        <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 90%;"> </div>
                    </div>
                    <hr>
                    <p>Safety</p>
                    <div class="progress">
                        <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 90%;"> </div>
                    </div>
                    <hr>
                    <p>FDA Approval</p>
                    <div class="progress">
                        <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;"> </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 features">
                <div class="panel-group" id="accordion">
                    <div class="panel">
                        <div class="panel-heading">
                            <h3 class="panel-title"> <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapseOne"> Latest News <i class="icon-plus"></i> <i class="icon-minus"></i> </a> </h3>
                        </div>
                        <div id="collapseOne" class="panel-collapse collapse in">
                            <div class="panel-body">
                                Information 1
                            </div>
                        </div>
                    </div>
                    <div class="panel">
                        <div class="panel-heading">
                            <h3 class="panel-title"> <a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo"> More Info <i class="icon-plus"></i> <i class="icon-minus"></i> </a> </h3>
                        </div>
                        <div id="collapseTwo" class="panel-collapse collapse">
                            <div class="panel-body">
                                Information 2
                            </div>
                        </div>
                    </div>
                    <div class="panel">
                        <div class="panel-heading">
                            <h3 class="panel-title"> <a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseThree"> More info <i class="icon-plus"></i> <i class="icon-minus"></i> </a> </h3>
                        </div>
                        <div id="collapseThree" class="panel-collapse collapse">
                            <div class="panel-body">
                                Information 3
                            </div>
                        </div>
                    </div>
                </div>
                <div class="visible-xs visible-sm">
                    <br class="gap-30" />
                    <hr class="gap-divider" />
                    <br class="gap-30" />
                </div>
            </div>
            <div class="col-lg-4 features">
                <h3>Our Mission</h3>
                <p>
                    We provide assay tests to customers with speed and efficiency.
                    <br />
                    <a href="@Url.Action("GetQuote", "Home")"> Enter your information here</a> to be contacted by a member of our team for a  complimentary quote.
                </p>
            </div>
        </div>
    </div>
</div>